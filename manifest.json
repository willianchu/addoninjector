{
  "name": "Injector",
  "version": "2.0",
  "manifest_version": 3,
  "description": "Injects a script into a page",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://*.google.com/*",
        "https://*.microsoft.com/*",
        "https://*.homedepot.ca/*"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}